{
    "name": "Youtube Loudener",
    "description": "Makes YouTube videos louder by applying a gain node to video audio",
    "version": "0.1.0",
    "manifest_version": 3,

    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*.youtube.com/*",
                "*://youtube.com/*"
            ],
            "js": ["content.js"],
            "run_at": "document_idle"
        }
    ],
    "permissions": ["storage"]
}



